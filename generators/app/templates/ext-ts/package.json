{
	"name": <%- JSON.stringify(name) %>,
	"version": "0.0.1",
	"description": <%- JSON.stringify(description) %>,
	"publisher": <%- JSON.stringify(publisher) %>,
	"main": "dist/main.js",
	"renderer": "dist/renderer.js",
	"engines": {
    "node": ">=14 <15",
		"lens": "^5.0.0"
  },
	"keywords": [
		"lens",
		"extension",
		"k8slens",
		"kubernetes"
	],
	"scripts": {
		"start": "NODE_ENV=development webpack --watch",
		"build": "npm run clean && NODE_ENV=production webpack",
		"clean": "rm -rf ./dist",
		"lint": "eslint",
		"test": "npm run lint && jest"
	},
	"dependencies": {
		<%- dep("react") %>,
		<%- dep("react-dom") %>
	},
	"devDependencies": {
		<%- dep("@k8slens/extensions") %>,
		<%- dep("ts-loader") %>,
		<%- dep("typescript") %>,
		<%- dep("@types/node") %>,
		<%- dep("@types/react") %>,
		<%- dep("@types/jest") %>,
		<%- dep("webpack") %>,
		<%- dep("webpack-cli") %>,
		<%- dep("eslint") %>,
		<%- dep("@typescript-eslint/eslint-plugin") %>,
		<%- dep("@typescript-eslint/parser") %>,
		<%- dep("jest") %>,
		<%- dep("@babel/preset-env") %>,
		<%- dep("@babel/preset-react") %>,
		<%- dep("@babel/preset-typescript") %>,
		<%- dep("@testing-library/jest-dom") %>,
		<%- dep("@testing-library/react") %>
	}
}
